@model IEnumerable<AwpDemo.Models.Asks>

@{
    ViewBag.Title = "Index";
}
<img src="~/image/logo.jpeg" style="float:right" width="100" height="100">
<h3>
    @Html.ActionLink("My question", "Myquestion")
</h3>
<br />

<p>
    <h2>@Html.ActionLink("Ask Medical Question ", "AsqUser")</h2>

</p>







@foreach (var item in Model)
{
<pre>
<b> @Html.DisplayNameFor(model => model.Question):</b>   @Html.DisplayFor(modelItem => item.UserName) :  @Html.DisplayFor(modelItem => item.Question)
</pre>
<pre>
<b> Answar:</b> @Html.DisplayFor(modelItem => item.Answer)
</pre>
    <br />


}
@Html.ActionLink("Back to Home page", "Index1", "Home")
<div>
    <h3> online user :</h3>@ViewBag.onlineusers
</div>
